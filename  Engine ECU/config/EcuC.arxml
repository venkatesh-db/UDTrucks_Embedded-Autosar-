                    <ADMIN-DATA>
                        <SDGS>
                            <SDG GID="DV:ECUInstance">
                                <SD GID="DV:ECUName">EngineECU</SD>
                                <SD GID="DV:ECUPartNumber">ECU-ENG-001</SD>
                                <SD GID="DV:ECUVersion">1.0.0</SD>
